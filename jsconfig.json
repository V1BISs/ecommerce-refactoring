{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "baseUrl": "src",
    "paths": {
      "*": [
        "*"
      ],
    }
  },
  "jsx": "react-jsx",
  "allowJs": true,
  "checkJs": false,
  "moduleResolution": "Node",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
}